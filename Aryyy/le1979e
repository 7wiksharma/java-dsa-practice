class Solution {
    public int findGCD(int[] nums) {
        Arrays.sort(nums);
        int n = nums.length;
        int largest = nums[n-1];
        int smallest = nums[0];
        int gcd=0;
        for(int i = 1;i<=smallest;i++)
        {
             if(smallest%i==0 && largest%i==0)
             {
                gcd=i;
             }


        }
        return gcd;
    }
}
