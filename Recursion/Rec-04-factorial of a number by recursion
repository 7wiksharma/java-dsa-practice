/// factorial of a number by recurison


public class Main
{
    public static int facto(int n)
    {
        if(n==0  || n==1)
        {
            return 1;
        }
        return n * facto(n-1);
    }
	public static void main(String[] args) {
		int n = 5;
		int ans = facto(n);
		System.out.println(ans);
	}
}
