
Rec-07- climb stairs 
leetcode - 70 
easy 

// note this is not optiml sol ... for optimization use dp here ...
class Solution {
    public int climbStairs(int n) {
                if(n==1 || n==0)
                {
                    return 1;
                }
                return climbStairs(n-1) + climbStairs(n-2);
    }
}
