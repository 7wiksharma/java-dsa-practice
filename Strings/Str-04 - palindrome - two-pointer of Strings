
public class Main
{
    //palindrome String 
    public static boolean ispalindrome(String s )
    {
        int i =0;
        int j = s.length()-1;
        
        while(i<j)
        {
            if(s.charAt(i)!=s.charAt(j)) 
            return false;
            else {
            i++;
            j--;
            }
            
        }
        return true;
    }
	public static void main(String[] args) {
	//	System.out.println("Hello World");
	System.out.print(ispalindrome("level"));
	}
}
