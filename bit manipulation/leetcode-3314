class Solution {
    public int[] minBitwiseArray(List<Integer> nums) {
        int n = nums.size();
        int ans[] = new int[n];
        //int j =0;
        for(int j =0;j<n;j++)
        {
            int target = nums.get(j);
              ans[j]=-1;
        for(int i =0;i<=target;i++)
        {
            
            if((i|(i+1)) == target) {
            ans[j]=i;
            break;
            }
        }
        }
        return ans;
    }
}
